

(define (problem BW-rand-10)
(:domain blocksworld)
(:objects b1 b2 b3 b4 b5 b6 b7 b8 b9 b10 )
(:init
(on-table b1)
(on b2 b9)
(on b3 b2)
(on b4 b5)
(on b5 b7)
(on-table b6)
(on b7 b10)
(on-table b8)
(on b9 b6)
(on b10 b1)
(clear b3)
(clear b4)
(clear b8)
)
(:goal
(and
(on b1 b7)
(on b4 b3)
(on b6 b1)
(on b7 b9)
(on b8 b5)
(on b9 b10)
(on b10 b4))
)
)
