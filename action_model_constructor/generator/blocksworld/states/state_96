

(define (problem BW-rand-10)
(:domain blocksworld)
(:objects b1 b2 b3 b4 b5 b6 b7 b8 b9 b10 )
(:init
(on-table b1)
(on b2 b1)
(on b3 b4)
(on b4 b2)
(on b5 b7)
(on b6 b10)
(on b7 b6)
(on-table b8)
(on b9 b5)
(on-table b10)
(clear b3)
(clear b8)
(clear b9)
)
(:goal
(and
(on b1 b2)
(on b3 b8)
(on b4 b9)
(on b5 b10)
(on b7 b1)
(on b8 b5)
(on b9 b7)
(on b10 b6))
)
)


