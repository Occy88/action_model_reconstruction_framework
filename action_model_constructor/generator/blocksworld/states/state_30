

(define (problem BW-rand-10)
(:domain blocksworld)
(:objects b1 b2 b3 b4 b5 b6 b7 b8 b9 b10 )
(:init
(on b1 b2)
(on-table b2)
(on b3 b1)
(on-table b4)
(on b5 b10)
(on b6 b8)
(on-table b7)
(on-table b8)
(on b9 b7)
(on b10 b9)
(clear b3)
(clear b4)
(clear b5)
(clear b6)
)
(:goal
(and
(on b1 b3)
(on b4 b8)
(on b5 b10)
(on b6 b5)
(on b7 b1)
(on b8 b7)
(on b9 b4)
(on b10 b2))
)
)


