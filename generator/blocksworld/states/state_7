

(define (problem BW-rand-10)
(:domain blocksworld)
(:objects b1 b2 b3 b4 b5 b6 b7 b8 b9 b10 )
(:init
(on b1 b5)
(on-table b2)
(on b3 b6)
(on b4 b2)
(on b5 b3)
(on b6 b7)
(on b7 b8)
(on b8 b9)
(on-table b9)
(on b10 b4)
(clear b1)
(clear b10)
)
(:goal
(and
(on b3 b10)
(on b4 b6)
(on b5 b3)
(on b6 b1)
(on b7 b4)
(on b8 b7)
(on b10 b8))
)
)


