

(define (problem BW-rand-10)
(:domain blocksworld)
(:objects b1 b2 b3 b4 b5 b6 b7 b8 b9 b10 )
(:init
(on b1 b2)
(on b2 b3)
(on b3 b10)
(on b4 b5)
(on-table b5)
(on-table b6)
(on b7 b4)
(on-table b8)
(on b9 b1)
(on b10 b6)
(clear b7)
(clear b8)
(clear b9)
)
(:goal
(and
(on b3 b1)
(on b4 b7)
(on b6 b2)
(on b8 b3)
(on b9 b8)
(on b10 b5))
)
)


