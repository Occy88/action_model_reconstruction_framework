

(define (problem BW-rand-10)
(:domain blocksworld)
(:objects b1 b2 b3 b4 b5 b6 b7 b8 b9 b10 )
(:init
(on b1 b9)
(on b2 b8)
(on b3 b6)
(on b4 b1)
(on-table b5)
(on-table b6)
(on b7 b10)
(on-table b8)
(on b9 b3)
(on b10 b4)
(clear b2)
(clear b5)
(clear b7)
)
(:goal
(and
(on b2 b8)
(on b3 b2)
(on b4 b6)
(on b5 b7)
(on b7 b3)
(on b8 b10)
(on b9 b5)
(on b10 b1))
)
)
